<div class="container" id="inbox_provider_form">
  <div class="row">
    <div class="col-md-12">
      <h3 class="title-inline">General Agencies</h3>
    </div>
  </div>
  <div class="row">
    <div class="col-md-12">
    <div class="table-border table-responsive">
      <table class="table">
      <% if @general_agency_profiles.present? %>
        <thead>
          <th>Legal Name</th>
          <th>dba</th>
          <th>Fein</th>
          <th>Entity Kind</th>
          <th>Market Kind</th>
          <th></th>
        </thead>
        <% @general_agency_profiles.each do |general_agency_profile| %>
          <tbody>
            <td><%= link_to general_agency_profile.legal_name, general_agencies_profile_path(general_agency_profile) %></td>
            <td><%= general_agency_profile.dba %></td>
            <td><%= general_agency_profile.fein %></td>
            <td><%= general_agency_profile.entity_kind.titleize %></td>
            <td><%= general_agency_profile.market_kind.titleize %></td>
            <td>
              <%= link_to "Manage for employers(#{general_agency_profile.employer_clients_count})", manage_employers_broker_agencies_profile_path(id: @broker_agency_profile.id, general_agency_profile_id: general_agency_profile.id), remote: true, class: 'btn btn-min' %>
            </td>
          </tbody>
        <% end %>
      <% else %>
        <tr><td>No General Agency Profiles found.</td></tr>
      <% end %>
      </table>
    </div>
   </div>
  </div>
</div>
